        "menu-item#shop__contact",
        "menu-item#shop__buy",
        "menu-item#shop__terms&conditions",
        "menu-item#shop__policies",
        "menu-item#shop__commitment"
      ],
      "props": {
        "orientation": "vertical",
        "blockClass":"footer__shop"
      }
    },
